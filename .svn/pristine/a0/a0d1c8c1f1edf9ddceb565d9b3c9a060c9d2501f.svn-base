@CHARSET "UTF-8";
nav {
	background-color: #A6A6A6;
	width: 250px;
	height: 900px;
	float: left;
}

ul#navi {
	margin: 0px;
	width: 250px;
	height: 827px;
	background-color: #A6A6A6;
	text-indent: 10px;
	padding: 0;
	font-family: "맑은 고딕";
	font-weight: bold;
}

ul#navi, ul#navi ul {
	margin: 0;
	padding: 0;
	list-style: none;
}

li.group {
	margin-bottom: 5px;
}

li.group div.title {
	height: 35px;
	line-height: 40px;
	background: #A6A6A6;
	cursor: pointer;
	line-height: 35px;
	background: #A6A6A6;
	font-weight: bold;
}
li.group div.title a.result{
	text-decoration: none;
	color: black;
	font-weight: bold;
}

ul.sub li {
	margin-bottom: 2px;
	height: 40px;
	line-height: 35px;
	background: #5E5E5E;
	cursor: pointer;
	color: #FFFFFF;
}
ul.sub li a {
        display: block;
        width: 100%;
        height:100%;
        text-decoration:none;
        color:#FFFFFF;
}
 ul.sub li:hover {
        background:#FFFFFF;
        
}
ul.sub li:hover a{
		color:#A6A6A6; 
	}